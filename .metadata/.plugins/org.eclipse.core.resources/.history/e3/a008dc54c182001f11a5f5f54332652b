package DataTypes;

public class Single_dim_2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a[]= {100,200,300,400,500,600,700,800,900}; 
		 
		System.out.println( a
				
				.length);
		
		for (int i=0; i <= a.length-1; i++ )  {
			System.out.println(i);
		}
	

	}

}
