package DataTypes;
public class Variables {

	public static void main(String[] args) 
	{		
		// Primitive Data Types  
		
		int a = 30 ; // Integer Data Type  
		char b = 'u';  // Character Data Type
		double c = 123.45; // Double or Float Data Type
		boolean d = true ;
		int t = 10;
		int u = 12;
		zv = t + u ;
	// Derived Data Types 
		String e = "Hello Upendar "; // String Data Types 
      
		
	//Output 
		
	  System.out.println(a);
      System.out.println(b);      
      System.out.println(c);      
      System.out.println(d);
      System.out.println(e);

		

	}

}

