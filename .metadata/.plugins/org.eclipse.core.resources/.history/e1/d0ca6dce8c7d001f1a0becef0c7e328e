package DataTypes;

public class Variables {

	public static void main(String[] args) 
	{		
		// Data Types 
		int a = 30 ; // Integer Data Type  
		char b = 'u';  // Character Data Type
		double c = 123.45; // Double or Float Data Type
		boolean d = true ;
		String e = "Hello Upendar "; // Derived Data Types 
      System.out.println(Hello);
      
	}

}
