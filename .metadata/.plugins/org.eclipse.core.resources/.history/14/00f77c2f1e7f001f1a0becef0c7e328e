package DataTypes;

public class Control_Statements {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int sum=10;
		
		if (sum%2==0) 
		{
		
			System.out.println("it is a even number");
		
		}
		else {
			
			System.out.println("it is a even number");
		}

	}

}
