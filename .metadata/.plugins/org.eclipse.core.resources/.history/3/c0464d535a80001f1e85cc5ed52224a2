package DataTypes;

public class While_Loop {

    public static void main(String[] args) {
        // Initialize a variable 'i' with value 1
        int i = 1;
        
        // A while loop that runs as long as 'i' is less than or equal to 500
        while (i <= 500) 
        {
            // Print the value of 'i' without a newline
            System.out.print(i);
            
            // Increment the value of 'i' by 1
            i++;
        }
    }
}
